{
  "name": "react-redux-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js -p",
    "start": "webpack-dev-server --history-api-fallback",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --require ./test/dom.js \"test/**/*spec.+(js|jsx)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aifarfa/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "pasit.r",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aifarfa/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/aifarfa/react-redux-boilerplate#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "testdouble": "^1.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "__coverage__"
        ]
      }
    },
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "syntax-object-rest-spread",
      "transform-object-rest-spread"
    ]
  }
}
